{
    "name": "whitegfx/laravel-validation",
    "description": "",
    "keywords": [
        "Laravel"
    ],
    "authors": [
        {
            "name": "Petr Votoƒçek",
            "email": "petr.votocek@gfx.cz"
        }
    ],
    "homepage": "https://github.com/whitegfx/laravel-validation",
    "require": {
        "php": "^7.2",
        "laravel/framework": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^5.0",
        "phpstan/phpstan": "^0.12.9"
    },
    "license": "MIT",
    "type": "package",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Whitegfx\\LaravelValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Whitegfx\\LaravelValidation\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Whitegfx\\LaravelValidation\\Providers\\LaravelValidationServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
        "analysis": "vendor/bin/phpstan analyse"
    }
}
